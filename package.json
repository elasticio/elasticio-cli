{
  "name": "elasticio-cli",
  "description": "Command-line interface for elastic.io components",
  "version": "2.1.0",
  "engines": {
    "node": ">= 12"
  },
  "scripts": {
    "pretest": "eslint lib spec spec-integration elasticio.js --fix --ignore-pattern spec-integration/test-component/ --ignore-pattern spec/test/",
    "test": "mocha spec/. spec-integration/.",
    "coverage": "nyc --reporter=html --reporter=text mocha spec/. spec-integration/."
  },
  "bin": {
    "elasticio": "./elasticio.js"
  },
  "main": "./elasticio.js",
  "dependencies": {
    "@elastic.io/component-commons-library": "1.1.0",
    "caporal": "git://github.com/elasticio/Caporal.js#92c446e74edb3e489985292604f24dc56a64f205",
    "chalk": "3.0.0",
    "dotenv": "8.2.0",
    "dotenv-extended": "2.7.1",
    "expand-tilde": "2.0.2",
    "handlebars": "4.5.3",
    "hjson": "3.2.1",
    "inquirer": "7.1.0",
    "jsonschema": "1.2.5",
    "lodash": "4.17.15",
    "properties-parser": "0.3.1",
    "remove-trailing-slash": "^0.1.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "eslint": "6.7.2",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "mocha": "6.2.2",
    "nyc": "14.1.1",
    "sinon": "7.5.0"
  }
}
